{
  "name": "@bobaverse/contracts",
  "version": "1.0.0",
  "description": "Contract repository for BobaVerse",
  "author": "DirtyCajunRice",
  "license": "MIT",
  "repository": "bobaverse/contracts",
  "dependencies": {
    "ts-node": "10.9.1",
    "typescript": "4.9.3",
    "hardhat": "2.13.0",
    "@openzeppelin/hardhat-upgrades": "1.23.1",
    "@nomicfoundation/hardhat-toolbox": "2.0.2",
    "@openzeppelin/contracts-upgradeable": "^4.8.3",
    "@nomicfoundation/hardhat-verify": "1.0.0"
  },
  "devDependencies": {
    "@dirtycajunrice/contracts": "^0.11.0",
    "@dirtycajunrice/hardhat-tasks": "^1.3.0"
  },
  "scripts": {
    "-------BASE--------": "",
    "compile": "npx hardhat compile",
    "------DEPLOY------": "",
    "deploy:BobaEth": "npx hardhat --network bobaEth deploy",
    "deploy:BobaOpera": "npx hardhat --network bobaOpera deploy",
    "deploy:BobaAvax": "npx hardhat --network bobaAvax deploy",
    "deploy:BobaBnb": "npx hardhat --network bobaBnb deploy",
    "-----UPGRADE-----": "",
    "upgrade:BobaEth": "npx hardhat --network bobaEth upgrade",
    "upgrade:BobaOpera": "npx hardhat --network bobaOpera upgrade",
    "upgrade:BobaAvax": "npx hardhat --network bobaAvax upgrade",
    "upgrade:BobaBnb": "npx hardhat --network bobaBnb upgrade",
    "-------VERIFY------": "",
    "verify:BobaEth": "npx hardhat --network bobaEth verify",
    "verify:BobaOpera": "npx hardhat --network bobaOpera verify",
    "verify:BobaAvax": "npx hardhat --network bobaAvax verify",
    "verify:BobaBnb": "npx hardhat --network bobaBnb verify",
    "-------BobaEth------": "",
    "TuringHelper:BobaEth:deploy": "npm run deploy:BobaEth -- --name TuringHelper",
    "TuringHelper:BobaEth:upgrade": "npm run upgrade:BobaEth -- --name TuringHelper",
    "TuringHelper:BobaEth:verify": "npm run verify:BobaEth -- --name TuringHelper",
    "BobaVerseArcade:BobaEth:deploy": "npm run deploy:BobaEth -- --name BobaVerseArcade",
    "BobaVerseArcade:BobaEth:upgrade": "npm run upgrade:BobaEth -- --name BobaVerseArcade",
    "BobaVerseArcade:BobaEth:verify": "npm run verify:BobaEth -- --name BobaVerseArcade",
    "-------BobaOpera------": "",
    "TuringHelper:BobaOpera:deploy": "npm run deploy:BobaOpera -- --name TuringHelper",
    "TuringHelper:BobaOpera:upgrade": "npm run upgrade:BobaOpera -- --name TuringHelper",
    "TuringHelper:BobaOpera:verify": "npm run verify:BobaOpera -- --name TuringHelper",
    "BobaVerseArcade:BobaOpera:deploy": "npm run deploy:BobaOpera -- --name BobaVerseArcade",
    "BobaVerseArcade:BobaOpera:upgrade": "npm run upgrade:BobaOpera -- --name BobaVerseArcade",
    "BobaVerseArcade:BobaOpera:verify": "npm run verify:BobaOpera -- --name BobaVerseArcade",
    "-------BobaAvax------": "",
    "TuringHelper:BobaAvax:deploy": "npm run deploy:BobaAvax -- --name TuringHelper",
    "TuringHelper:BobaAvax:upgrade": "npm run upgrade:BobaAvax -- --name TuringHelper",
    "TuringHelper:BobaAvax:verify": "npm run verify:BobaAvax -- --name TuringHelper",
    "BobaVerseArcade:BobaAvax:deploy": "npm run deploy:BobaAvax -- --name BobaVerseArcade",
    "BobaVerseArcade:BobaAvax:upgrade": "npm run upgrade:BobaAvax -- --name BobaVerseArcade",
    "BobaVerseArcade:BobaAvax:verify": "npm run verify:BobaAvax -- --name BobaVerseArcade",
    "-------BobaBnb------": "",
    "TuringHelper:BobaBnb:deploy": "npm run deploy:BobaBnb -- --name TuringHelper",
    "TuringHelper:BobaBnb:upgrade": "npm run upgrade:BobaBnb -- --name TuringHelper",
    "TuringHelper:BobaBnb:verify": "npm run verify:BobaBnb -- --name TuringHelper",
    "BobaVerseArcade:BobaBnb:deploy": "npm run deploy:BobaBnb -- --name BobaVerseArcade",
    "BobaVerseArcade:BobaBnb:upgrade": "npm run upgrade:BobaBnb -- --name BobaVerseArcade",
    "BobaVerseArcade:BobaBnb:verify": "npm run verify:BobaBnb -- --name BobaVerseArcade"
  }
}
